<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converters="using:DefaultApplication.DockingLayout.Internal.Converters"
    xmlns:local="using:DefaultApplication.DockingLayout.Internal.Controls"
    x:ClassModifier="internal">

    <ControlTheme x:Key="{x:Type local:LayoutContentPresenter}" TargetType="local:LayoutContentPresenter">

        <Setter Property="Template">
            <ControlTemplate>
                <ContentPresenter Content="{TemplateBinding Content, Mode=TwoWay}" />
            </ControlTemplate>
        </Setter>

    </ControlTheme>

</ResourceDictionary>
